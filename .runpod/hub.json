{
  "title": "ComfyUI Wan 2.2 Serverless I2V",
  "description": "Production-ready serverless template for Wan 2.2 Image-to-Video generation with adaptive GPU optimization and 3-stage workflow",
  "category": "video",
  "deployment": "serverless",
  "icon": "https://raw.githubusercontent.com/Gymbo-007/comfyui-wan22-serverless_cuda12.8/main/assets/icon.png",
  "tags": ["image-to-video", "wan-2.2", "comfyui", "video-generation", "ai", "serverless"],
  "author": "Gymbo-007",
  "version": "1.0.0",
  "license": "MIT",
  "readme": "README.md",
  "handler": "handler.py",
  "machine": {
    "gpu": {
      "min_vram": 20,
      "recommended_vram": 24,
      "supports": ["RTX 4090", "RTX 5090", "RTX 6000 Ada", "A5000", "H100"]
    },
    "cpu": {
      "min_cores": 4,
      "recommended_cores": 8
    },
    "memory": {
      "min_ram": 16,
      "recommended_ram": 32
    },
    "disk_size": 50
  },
  "environment_variables": {
    "DEFAULT_QUANT": {
      "type": "string",
      "default": "Q5_K_S",
      "description": "Default GGUF quantization level (Q4_K_S, Q5_K_S, Q6_K, Q8_0)",
      "options": ["Q4_K_S", "Q5_K_S", "Q6_K", "Q8_0"]
    },
    "MAX_RESOLUTION": {
      "type": "numeric",
      "default": 832,
      "description": "Maximum output resolution (pixels)",
      "min": 256,
      "max": 1280
    },
    "SAGE_ATTENTION_IMPL": {
      "type": "string",
      "default": "triton",
      "description": "SageAttention implementation",
      "options": ["triton", "auto", "disabled"]
    },
    "PYTORCH_CUDA_ALLOC_CONF": {
      "type": "string",
      "default": "max_split_size_mb:512,expandable_segments:True",
      "description": "PyTorch CUDA memory allocation configuration"
    }
  },
  "input_schema": {
    "image": {
      "type": "string",
      "description": "Base64 encoded input image",
      "required": true
    },
    "prompt": {
      "type": "string",
      "description": "Text prompt describing the desired video motion",
      "default": "high quality video, smooth motion",
      "required": false
    },
    "negative_prompt": {
      "type": "string", 
      "description": "Negative prompt to avoid in generation",
      "default": "static, blurry, low quality",
      "required": false
    },
    "resolution": {
      "type": "integer",
      "description": "Output resolution (long side in pixels)",
      "default": 832,
      "min": 256,
      "max": 1280,
      "required": false
    },
    "length": {
      "type": "integer",
      "description": "Video length in frames",
      "default": 25,
      "min": 8,
      "max": 81,
      "required": false
    }
  },
  "output_schema": {
    "videos": {
      "type": "array",
      "description": "Generated videos in base64 format",
      "items": {
        "filename": {
          "type": "string",
          "description": "Output video filename"
        },
        "video_base64": {
          "type": "string",
          "description": "Base64 encoded video data"
        }
      }
    },
    "prompt_id": {
      "type": "string",
      "description": "ComfyUI prompt ID for tracking"
    },
    "processing_time": {
      "type": "number",
      "description": "Total processing time in seconds"
    }
  },
  "performance": {
    "typical_inference_time": "30-60 seconds",
    "gpu_memory_usage": "18-22 GB",
    "concurrent_requests": 1
  }
}